pipeline {
    agent any
    tools {
        maven 'M2_HOME'
    }
        
    stages {
       
    } stage('build') {
            steps {
                echo 'Hello build'
                sh 'mvn clean'
                sh 'mvn' install'
                sh 'mvn package'
                
            }
        }
        
        stage('test') {
            steps {
                echo 'mvn test'
                sleep 5
            }
        }
        
        stage('deploy') {
            steps {
                echo 'Hello test'
                sleep 5
            }
        }
}    }
